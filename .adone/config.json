{
    "artifacts": {
        "share": [
            "share"
        ],
        "src": [
            "src"
        ],
        "src.app": [
            "src/app"
        ],
        "src.commands": [
            "src/commands"
        ],
        "src.common": [
            "src/glosses",
            "src/index.js"
        ],
        "src.tasks": [
            "src/tasks"
        ],
        "src.native": [
            "src/native"
        ],
        "lib.tasks": [
            "lib/tasks"
        ],
        "configs": [
            "etc",
            ".eslintignore",
            ".eslintrc.js",
            ".gitattributes",
            ".gitignore",
            ".npmignore",
            "jsconfig.json"
        ],
        "info": [
            "LICENSE",
            "README.md"
        ],
        "tests": [
            "tests",
            "shanifile.js"
        ]
    },
    "tasks": {
        "basePath": "lib/tasks",
        "default": false,
        "tags": {
            "pub": "pub",
            "own": "own"
        }
    },
    "mountPoints": "lib/mount_points",
    "scheme": {
        "tasks": {
            "description": "Realm tasks",
            "src": "src/tasks/**/*.js",
            "dst": "lib/tasks",
            "task": "transpile"
        },
        "mountPoints": {
            "description": "Realm mount points",
            "src": "src/mount_points/**/*.js",
            "dst": "lib/mount_points",
            "task": "transpile"
        },
        "assets": {
            "description": "All assets",
            "src": [
                "src/**/*",
                "!src/**/*.js",
                "!src/**/*.wat",
                "!src/glosses/schema/__/dot/*",
                "!src/**/native/**/*"
            ],
            "dst": "lib",
            "dstClean": [
                "lib/**/*",
                "!lib/**/*.js",
                "!lib/**/*.map",
                "!lib/**/*.node"
            ]
        },
        "app": {
            "description": "ADONE CLI application",
            "src": "src/app/adone.js",
            "dst": "bin",
            "task": "transpileExe",
            "scheme": {
                "internals": {
                    "description": "ADONE CLI internals",
                    "src": [
                        "!src/app/adone.js",
                        "src/app/**/*.js"
                    ],
                    "dst": "lib/app",
                    "task": "transpile"
                }
            }
        },
        "commands": {
            "description": "ADONE CLI commands",
            "src": "src/commands/**/*.js",
            "dst": "lib/commands",
            "task": "transpile"
        },
        "common": {
            "description": "ADONE common",
            "src": [
                "src/index.js",
                "src/common.js",
                "src/reflect.js"
            ],
            "dst": "lib",
            "task": "transpile"
        },
        "gloss": {
            "description": "Glosses - common foundation libraries",
            "src": [
                "src/glosses/**/*.js"
            ],
            "dst": "lib/glosses",
            "dstClean": [
                "lib/glosses/**/*.js",
                "lib/glosses/**/*.map"
            ],
            "scheme": {
                "app": {
                    "description": "Application framework",
                    "src": "src/glosses/app/**/*.js",
                    "dst": "lib/glosses/app",
                    "task": "transpile",
                    "scheme": {
                        "lockfile": {
                            "description": "Inter-process file locking implementation",
                            "src": "src/glosses/app/lockfile.js",
                            "dst": "lib/glosses/app",
                            "task": "transpile"
