{
    "tasks": {
        "basePath": "./lib/tasks"
    },
    "scheme": {
        "tasks": {
            "description": "Realm tasks",
            "src": "src/tasks/**/*.js",
            "dst": "lib/tasks",
            "task": "transpile"
        },
        "assets": {
            "description": "All assets",
            "src": [
                "src/**/*",
                "!src/**/*.js",
                "!src/**/*.wat",
                "!src/glosses/schema/__/dot/*",
                "!src/**/native/**/*"
            ],
            "dst": "lib",
            "dstClean": [
                "lib/**/*",
                "!lib/**/*.js",
                "!lib/**/*.map",
                "!lib/**/*.node"
            ]
        },
        "app": {
            "description": "ADONE cli utility",
            "src": "src/app/adone.js",
            "dst": "bin",
            "task": "transpileExe"
        },
        "index": {
            "description": "ADONE index file",
            "src": "src/index.js",
            "dst": "lib",
            "task": "transpile"
        },
        "gloss": {
            "description": "Glosses - common foundation libraries",
            "src": [
                "src/glosses/**/*.js"
            ],
            "dst": "lib/glosses",
            "dstClean": [
                "lib/glosses/**/*.js",
                "lib/glosses/**/*.map"
            ],
            "scheme": {
                "app": {
                    "description": "Application framework",
                    "src": "src/glosses/app/**/*.js",
                    "dst": "lib/glosses/app",
                    "task": "transpile",
                    "scheme": {
                        "logger": {
                            "description": "Application logger",
                            "src": "src/glosses/app/logger/**/*.js",
                            "dst": "lib/glosses/app/logger",
                            "task": "transpile"
                        },
                        "lockfile": {
                            "description": "Inter-process file locking implementation",
                            "src": "src/glosses/app/lockfile.js",
                            "dst": "lib/glosses/app",
                            "task": "transpile"
                        },
                        "report": {
                            "description": "Delivers a human-readable diagnostic summary, written to file",
                            "src": "src/glosses/app/report/**/*.js",
                            "dst": "lib/glosses/app/report",
                            "task": "transpile",
                            "native": {
                                "src": "src/glosses/app/report/native",
                                "dst": "lib/glosses/app/report/native"
                            }
                        }
                    }
                },
                "archives": {
                    "description": "Archivers",
                    "src": "src/glosses/archives/**/*.js",
                    "dst": "lib/glosses/archives",
                    "task": "transpile"
                },
                "assertion": {
                    "description": "Assertion utilites",
                    "src": "src/glosses/assertion/**/*.js",
                    "dst": "lib/glosses/assertion",
                    "task": "transpile",
                    "predecessor": [
                        "https://github.com/chaijs",
                        "https://github.com/domenic/sinon-chai",
                        "https://github.com/prodatakey/dirty-chai",
                        "https://github.com/domenic/chai-as-promised"
                    ]
                },
                "async": {
                    "description": "Async utilities for node and the browser",
                    "task": "copy",
                    "src": [
                        "src/glosses/async/**/*.js",
                        "!src/glosses/async/**/LICENSE",
                        "!src/glosses/async/**/README.md"
                    ],
                    "dst": "lib/glosses/async",
                    "original": "https://github.com/caolan/async"
                },
                "collection": {
                    "description": "Implementations of common collections",
                    "src": "src/glosses/collections/**/*.js",
                    "dst": "lib/glosses/collections",
