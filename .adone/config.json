{
    "struct": {
        "assets": {
            "description": "Assets",
            "src": [
                "src/**/*",
                "!src/**/*.js",
                "!src/**/*.wat",
                "!src/glosses/schema/__/dot/*",
                "!src/**/native/**/*"
