                    ],
                    "dst": "lib/cli",
                    "task": "adoneTranspile",
                    "namespace": "cli"
                },
                "index": {
                    "description": "Main index file",
                    "index": "index.js",
                    "src": "src/index.js",
                    "dst": "lib",
                    "task": "transpile"
