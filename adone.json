{
    "name": "adone",
    "description": "The generalized core of 'cyber-fractal systems' infrastructure.",
    "version": "0.6.71",
    "author": "Adone Core Team <info@adone.io>",
    "struct": {
        "assets": {
            "description": "Assets",
            "src": [
                "src/**/*",
                "!src/**/*.js",
                "!src/glosses/schema/__/dot/*",
                "!src/**/native/**/*"
            ],
            "dst": "lib",
            "dstClean": [
                "lib/**/*",
                "!lib/**/*.js",
                "!lib/**/*.map",
                "!lib/native/**/*"
            ]
        },
        "bin": {
            "description": "Adone CLI",
            "src": "src/cli/adone.js",
            "dst": "bin",
            "task": "adoneTranspileExe"
        },
        "lib": {
            "description": "Glosses and components",
            "src": [
                "src/**/*.js",
                "!src/cli/adone.js",
                "!src/native/**/*"
            ],
            "dst": "lib",
            "dstClean": [
                "lib/**/*.js",
                "lib/**/*.map"
            ],
            "namespace": "adone",
            "struct": {
                "dev": {
                    "namespace": "dev"
                },
                "cli": {
                    "description": "Adone CLI stuff",
                    "src": [
                        "src/cli/**/*",
                        "!src/cli/adone.js"
                    ],
                    "dst": "lib/cli",
                    "task": "adoneTranspile",
                    "namespace": "cli"
                },
                "index": {
                    "description": "Main index file",
                    "src": "src/index.js",
                    "dst": "lib",
                    "task": "transpile"
                },
                "application": {
                    "description": "Application framework",
                    "src": [
                        "src/glosses/application/**/*.js",
                        "!src/glosses/application/report/native/**/*"
                    ],
                    "dst": "lib/glosses/application",
                    "task": "transpile",
                    "namespace": "application",
                    "struct": {
                        "report": {
                            "description": "Delivers a human-readable diagnostic summary, written to file",
                            "namespace": "report",
                            "src": [
                                "src/glosses/application/report/**/*.js",
                                "!src/glosses/application/report/native/**/*"
                            ],
                            "dst": "lib/glosses/application/report",
                            "task": "transpile",
                            "native": {
                                "src": "src/glosses/application/report/native",
                                "dst": "lib/glosses/application/report/native"
                            }
                        }
                    }
                },
                "archives": {
                    "description": "Archivers",
                    "src": "src/glosses/archives/**/*.js",
                    "dst": "lib/glosses/archives",
                    "task": "adoneTranspile",
                    "namespace": "archives"
                },
                "assertion": {
                    "description": "Assertion utilites",
                    "src": "src/glosses/assertion/**/*.js",
                    "dst": "lib/glosses/assertion",
                    "task": "adoneTranspile",
                    "namespace": "assertion"
                },
                "collection": {
                    "description": "Implementations of common collections",
                    "src": "src/glosses/collections/**/*.js",
                    "dst": "lib/glosses/collections",
                    "task": "adoneTranspile",
                    "namespace": "collection"
                },
                "compressor": {
                    "description": "Compressors",
                    "namespace": "compressor",
                    "src": "src/glosses/compressors/index.js",
                    "dst": "lib/glosses/compressors",
                    "struct": {
                        "brotli": {
                            "description": "Brotli compression format",
                            "namespace": "brotli",
                            "src": [
                                "src/glosses/compressors/brotli/**/*.js",
                                "!src/glosses/compressors/brotli/native/**/*"
                            ],
                            "dst": "lib/glosses/compressors/brotli",
                            "task": "transpile",
                            "native": {
                                "src": "src/glosses/compressors/brotli/native",
                                "dst": "lib/glosses/compressors/brotli/native"
                            }
                        },
                        "deflate": {
                            "description": "Deflate compressor/decompressor",
                            "namespace": "deflate",
                            "task": "transpile",
                            "src": "src/glosses/compressors/deflate.js",
                            "dst": "lib/glosses/compressors"
                        },
                        "gz": {
                            "description": "Gzip compressor/decompressor",
                            "namespace": "gz",
                            "task": "transpile",
                            "src": "src/glosses/compressors/gzip.js",
                            "dst": "lib/glosses/compressors"
                        },
                        "lzma": {
                            "description": "LZMA compressor/decompressor",
                            "namespace": "lzma",
                            "task": "transpile",
                            "src": "src/glosses/compressors/lzma.js",
                            "dst": "lib/glosses/compressors"
                        },
                        "snappy": {
                            "description": "Snappy, a fast compressor/decompressor",
                            "namespace": "snappy",
                            "task": "transpile",
                            "src": [
                                "src/glosses/compressors/snappy/**/*.js",
                                "!src/glosses/compressors/snappy/native/**/*"
                            ],
                            "dst": "lib/glosses/compressors/snappy",
                            "native": {
                                "src": "src/glosses/compressors/snappy/native",
                                "dst": "lib/glosses/compressors/snappy/native"
                            }
                        },
                        "xz": {
                            "description": "XZ compressor/decompressor",
                            "namespace": "xz",
                            "task": "transpile",
                            "src": [
                                "src/glosses/compressors/xz/**/*.js",
                                "!src/glosses/compressors/xz/native/**/*"
                            ],
                            "dst": "lib/glosses/compressors/xz",
                            "native": {
                                "src": "src/glosses/compressors/xz/native",
                                "dst": "lib/glosses/compressors/xz/native"
                            }
                        }
                    }
                },
                "configuration": {
                    "description": "Configurations",
                    "src": "src/glosses/configurations/**/*.js",
                    "dst": "lib/glosses/configurations",
                    "task": "adoneTranspile",
                    "namespace": "configuration"
                },
                "crypto": {
                    "description": "Common cryptographic utilites",
                    "src": "src/glosses/crypto/**/*.js",
                    "dst": "lib/glosses/crypto",
                    "task": "transpile",
                    "namespace": "crypto",
                    "struct": {
                        "ed25519": {
                            "description": "Implementation of ed25519 signatures",
                            "namespace": "ed25519",
                            "src": "src/glosses/crypto/ed25519/**/*.js",
                            "dst": "lib/glosses/crypto/ed25519",
                            "task": "transpile",
                            "native": {
                                "src": "src/glosses/crypto/ed25519/native",
                                "dst": "lib/glosses/crypto/ed25519/native"
                            }
                        },
                        "secp256k1": {
                            "description": "Optimized implementation for EC operations on curve secp256k1",
                            "namespace": "secp256k1",
                            "src": "src/glosses/crypto/secp256k1/**/*.js",
                            "dst": "lib/glosses/crypto/secp256k1",
                            "task": "transpile",
                            "native": {
                                "src": "src/glosses/crypto/secp256k1/native",
                                "dst": "lib/glosses/crypto/secp256k1/native"
                            }
                        }
                    }
                },
                "data": {
                    "description": "Data generic manipulation utilites and serializers",
                    "src": "src/glosses/data/**/*.js",
                    "dst": "lib/glosses/data",
                    "dstClean": "lib/glosses/data/**/*",
                    "task": "transpile",
                    "namespace": "data",
                    "struct": {
                        "base64": {
                            "description": "Implementation of BASE64 serializer",
                            "index": "glosses/data/base64",
                            "src": "src/glosses/data/base64.js",
                            "namespace": "base64"
                        },
                        "bson": {
                            "description": "Implementation of BSON serializer",
                            "src": "src/glosses/data/bson/**/*.js",
                            "dst": "lib/glosses/data/bson",
                            "namespace": "bson",
                            "native": {
                                "src": "src/glosses/data/bson/native",
                                "dst": "lib/glosses/data/bson/native"
                            }
                        },
                        "json": {
                            "description": "Implementation of JSON serializers",
                            "src": "src/glosses/data/json/**/*.js",
                            "dst": "lib/glosses/data/json",
                            "namespace": "json"
                        },
                        "json5": {
                            "description": "Implementation of JSON5 serializer",
                            "index": "glosses/data/json5",
                            "src": "src/glosses/data/json5.js",
                            "namespace": "json5"
                        },
                        "mpak": {
                            "description": "Implementation of MessagePack serializer",
                            "index": "glosses/data/mpak",
                            "src": "src/glosses/data/mpak.js",
                            "namespace": "mpak"
                        },
                        "yaml": {
                            "description": "Implementation of YAML serializer",
                            "src": "src/glosses/data/yaml/**/*.js",
                            "dst": "lib/glosses/data/yaml",
                            "namespace": "yaml"
                        },
                        "base58": {
                            "description": "Implementation of Base58 serializer",
                            "index": "glosses/data/base58",
                            "src": "src/glosses/data/base58.js",
                            "namespace": "base58"
                        },
                        "varint": {
                            "description": "Implementation of Protobuf varint",
                            "index": "glosses/data/varint",
                            "src": "src/glosses/data/varint.js",
                            "namespace": "varint"
                        },
                        "varintSigned": {
                            "description": "Implementation of Protobuf signed varint",
                            "index": "glosses/data/varint_signed",
                            "src": "src/glosses/data/varint_signed.js",
                            "namespace": "varintSigned"
                        },
                        "protobuf": {
                            "description": "Implementation of Protocol Buffers",
                            "src": "src/glosses/data/protobuf/**/*.js",
                            "dst": "lib/glosses/data/protobuf",
                            "namespace": "protobuf"
                        }
                    }
                },
                "database": {
                    "description": "Databases",
                    "src": "src/glosses/databases/**/*.js",
                    "dst": "lib/glosses/databases",
                    "task": "transpile",
                    "namespace": "database",
                    "struct": {
                        "level": {
                            "description": "Level is a key/value database",
                            "namespace": "level",
                            "task": "transpile",
                            "src": "src/glosses/databases/level/**/*.js",
                            "dst": "lib/glosses/databases/level",
                            "struct": {
                                "backend": {
                                    "description": "Level's backends",
                                    "namespace": "backend",
                                    "task": "transpile",
                                    "src": [
                                        "src/glosses/databases/level/backends/**/*.js",
                                        "!src/glosses/databases/level/backends/leveldb/native/**/*"
                                    ],
                                    "dst": "lib/glosses/databases/level/backends",
                                    "native": {
                                        "src": "src/glosses/databases/level/backends/leveldb/native",
                                        "dst": "lib/glosses/databases/level/backends/leveldb/native"
                                    }
                                }
                            }
                        },
                        "redis": {
                            "description": "Redis is a key/value database",
                            "namespace": "redis",
                            "task": "transpile",
                            "src": "src/glosses/databases/redis/**/*.js",
                            "dst": "lib/glosses/databases/redis",
                            "native": {
                                "src": "src/glosses/databases/redis/__/redis/libparser/native",
                                "dst": "lib/glosses/databases/redis/__/redis/libparser/native"
                            }
                        }
                    }
                },
                "datastore": {
                    "description": "Datastores",
                    "task": "adoneTranspile",
                    "src": "src/glosses/datastores/**/*.js",
                    "dst": "lib/glosses/datastores",
                    "namespace": "datastore"
                },
                "datetime": {
                    "description": "Datetime utilites",
                    "task": "adoneTranspile",
                    "src": "src/glosses/datetime/**/*.js",
                    "dst": "lib/glosses/datetime",
                    "namespace": "datetime"
                },
                "diff": {
                    "description": "Text differencing",
                    "task": "adoneTranspile",
                    "src": "src/glosses/diff/**/*.js",
                    "dst": "lib/glosses/diff",
                    "namespace": "diff"
                },
                "event": {
                    "description": "Event emitters and other event utilites",
                    "task": "adoneTranspile",
                    "src": "src/glosses/events/**/*.js",
                    "dst": "lib/glosses/events",
                    "namespace": "event"
                },
                "x": {
                    "description": "Exceptions and helpers",
                    "task": "adoneTranspile",
                    "src": "src/glosses/exceptions/**/*.js",
                    "dst": "lib/glosses/exceptions",
                    "namespace": "x"
                },
                "fs": {
                    "description": "Filesystem utilites",
                    "task": "transpile",
                    "src": [
                        "src/glosses/fs/**/*.js",
                        "!src/glosses/fs/fuse/native/**/*"
                    ],
                    "dst": "lib/glosses/fs",
                    "namespace": "fs",
                    "struct": {
                        "fuse": {
                            "description": "FUSE binding",
                            "namespace": "fuse",
                            "task": "transpile",
                            "src": "src/glosses/fs/fuse/**/*.js",
                            "dst": "lib/glosses/fs/fuse",
                            "native": {
                                "src": "src/glosses/fs/fuse/native",
                                "dst": "lib/glosses/fs/fuse/native"
                            }
                        }
                    }
                },
                "geoip": {
                    "description": "Geolocation utilites",
                    "task": "adoneTranspile",
                    "src": "src/glosses/geoip/**/*.js",
                    "dst": "lib/glosses/geoip",
                    "namespace": "geoip"
                },
                "gyp": {
                    "description": "Node.js native addon build tool",
                    "namespace": "gyp",
                    "index": "js:index.js",
                    "struct": {
                        "js": {
                            "task": "transpile",
                            "src": [
                                "src/glosses/gyp/**/*.js",
                                "!src/glosses/gyp/gyp/**/*"
                            ],
                            "dst": "lib/glosses/gyp"
                        },
                        "native-gyp": {
                            "src": "src/glosses/gyp/gyp/**/*",
                            "dst": "lib/glosses/gyp/gyp"
                        }
                    }
                },
                "hardware": {
                    "description": "Hardware stuff",
                    "task": "adoneTranspile",
                    "src": "src/glosses/hardware/**/*.js",
                    "dst": "lib/glosses/hardware",
                    "namespace": "hardware",
                    "struct": {
                        "board": {
                            "description": "Development boards",
                            "namespace": "board",
                            "task": "transpile",
                            "src": "src/glosses/hardware/boards/**/*.js",
                            "dst": "lib/glosses/hardware/boards",
                            "struct": {
                                "rpi": {
                                    "description": "SaspberryPI board",
