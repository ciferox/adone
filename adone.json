{
    "name": "adone",
    "description": "The generalized core of 'cyber-fractal systems' infrastructure.",
    "version": "0.6.71",
    "author": "Adone Core Team <info@adone.io>",
    "struct": {
        "assets": {
            "description": "Assets",
            "src": [
                "src/**/*",
                "!src/**/*.js",
                "!src/glosses/schema/__/dot/*",
                "!src/**/native/**/*"
            ],
            "dst": "lib",
            "dstClean": [
                "lib/**/*",
                "!lib/**/*.js",
                "!lib/**/*.map",
                "!lib/native/**/*"
            ]
        },
        "bin": {
            "description": "Adone CLI",
            "src": "src/cli/adone.js",
            "dst": "bin",
            "task": "adoneTranspileExe"
        },
        "lib": {
            "description": "Glosses and components",
            "index": "index.js",
            "src": [
                "src/**/*.js",
                "!src/cli/adone.js"
            ],
            "dst": "lib",
            "dstClean": [
                "lib/**/*.js",
                "lib/**/*.map"
            ],
            "namespace": "adone",
