{
	"compilerOptions": {
		"allowSyntheticDefaultImports": true,
		"diagnostics": true,
		"lib": ["es6", "dom"],
		"module": "commonjs",
		"noEmitOnError": true,
		"noUnusedLocals": true,
		"noUnusedParameters": true,
		"pretty": true,
		"skipLibCheck": true,
		"strictPropertyInitialization": false,
		"strictFunctionTypes": false,
		"strict": true,
		"target": "es2015"
	}
}
